{
  "dataset_reader": {
    "class_name": "dp:T5DatasetReader",
    "name": "{TASK}",
    "shuffle": true,
    "train": "train",
    "valid": null,
    "test": null
  },
  "dataset_iterator": {
    "class_name": "dp:T5TFDatasetIterator"
  },
  "chainer": {
    "in": ["x"],
    "in_y": ["y"],
    "pipe": [
      {
        "class_name": "dp:TorchTransformersPreprocessor",
        "id": "preprocessor",
        "vocab_file": "t5-base",
        "do_lower_case": false,
        "max_seq_length": 256,
        "reduce_pad": true,
        "in": ["x"],
        "out": ["x_features"]
      },
      {
        "in": ["y"],
        "out": ["y_features"],
        "ref": "preprocessor"
      },
      {
        "class_name": "dp:T5Text2TextModel",
        "pretrained_model": "{PRETRAINED_PATH}",
        "t5_configs_path": "/home/kuratov/t5-experiments/",        
        "check_commit": false,
        "sub_batch_size": 32,
        "max_generation_len": 10,
        "save_path": "{MODEL_PATH}/model",
        "load_path": "{MODEL_PATH}/model",
        "optimizer": "AdamW",
        "optimizer_parameters": {
          "lr": 2e-05
        },
        "learning_rate_drop_patience": 5,
        "learning_rate_drop_div": 2.0,
        "in": ["x_features"],
        "in_y": ["y_features"],
        "out": ["y_pred"]
      }
    ],
    "out": ["y_pred"]
  },
  "train": {
    "batch_size": 32,
    "log_every_n_batches": 250,
    "log_on_k_batches": 50,
    "save_every_n_batches": 500,
    "metrics": [],
    "show_examples": false,
    "validate_first": false,
    "evaluation_targets": [],
    "class_name": "torch_trainer",
    "tensorboard_log_dir": "{MODEL_PATH}/",
    "pytest_max_batches": 2
  },
  "metadata": {
    "imports": ["dp"],
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "PRETRAINED_PATH": "./runs/base_wiki_bs_128/",
      "MODEL_PATH": "{PRETRAINED_PATH}/glue/mixture/bs_32",
      "TASK": "glue_v002_proportional"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/pytorch.txt",
      "{DEEPPAVLOV_PATH}/requirements/transformers.txt"
    ]
  }
}
